# Production environment variables for Docker build
# These get baked into the React bundle during 'npm run build'

# Backend is on same origin when served from Docker
# Use empty string to make API calls relative (e.g., /api/health instead of http://localhost:8000/api/health)
VITE_BACKEND_URL=

# Canvas configuration (optional defaults - can be overridden in UI)
VITE_CANVAS_API_URL=
VITE_CANVAS_API_KEY=
VITE_CANVAS_COURSE_ID=
