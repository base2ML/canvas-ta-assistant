# Canvas API Configuration
# Copy this file to .env and fill in your actual values
# These are default values that can be overridden in the UI

# Canvas instance URL (e.g., https://your-school.instructure.com)
VITE_CANVAS_API_URL=https://your-school.instructure.com

# Canvas API token (generate in Canvas: Account → Settings → Approved Integrations → New Access Token)
VITE_CANVAS_API_KEY=your-canvas-api-token-here

# Course ID(s) - single ID or comma-separated list (find in Canvas URLs like /courses/12345)
VITE_CANVAS_COURSE_ID=12345

# Backend FastAPI server URL
VITE_BACKEND_URL=http://localhost:8000